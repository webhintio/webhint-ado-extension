{
    "manifestVersion": 1,
    "id": "webhint-devops",
    "publisher": "<publisher-id>",
    "version": "1.0.7",
    "name": "Webhint",
    "description": "ADO extension for Webhint",
    "public": false,
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
      "default": "static/images/extension-logo.png"
    },
    "contributions": [
        {
            "id": "webhint-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/Webhint"
            }
        }
    ],
    "files": [
        {
          "path": "static/images/extension-logo.png",
          "packagePath": "images/extension-logo.png",
          "addressable": true
        },
        {
          "path": "static/images/screenshot-1.png",
          "packagePath": "images/screenshot-1.png",
          "addressable": true
        },
        {
          "path": "static/images/screenshot-2.png",
          "packagePath": "images/screenshot-2.png",
          "addressable": true
        },
        {
          "path": "static/images/task-icon.png",
          "packagePath": "Tasks/Webhint/icon.png",
          "addressable": true
        },
        {
          "path": "task/src/task.json",
          "packagePath": "Tasks/Webhint/task.json",
          "addressable": true
        },
        {
          "path": "task/src/execute-webhint.js",
          "packagePath": "Tasks/Webhint/execute-webhint.js",
          "addressable": true
        },
        {
          "path": "task/node_modules",
          "packagePath": "Tasks/Webhint/node_modules",
          "addressable": true
        },
        {
          "path": "node_modules/vss-web-extension-sdk/lib",
          "packagePath": "lib",
          "addressable": true
        }
      ]
}